type User {
   id: ID,
   name: String,
   email: String
}

input UserDto {
   name: String,
   email: String
}

type Mutation {
   createUser(user: UserDto): User
   updateUser(id: ID!, user: UserDto): User
   deleteUserById(id: ID!): DeleteUserPayload
}

type Query {
   getUserById(id: ID!): User
   getAllUsers: [User]
}

type DeleteUserPayload {
   msg: String
}